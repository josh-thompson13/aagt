{
  "name": "aagt-private-loans",
  "version": "3.67.2",
  "author": "AAGT Private Loans",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && bun run post-build",
    "post-build": "bun run copy-to-root",
    "copy-to-root": "node scripts/copy-to-root.js",
    "start": "next start",
    "clean": "rm -rf .next out node_modules",
    "check-types": "tsc --noEmit --pretty",
    "export": "next build",
    "fresh-install": "bun run clean && bun install",
    "lint": "biome lint --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "type-check": "tsc --noEmit",
    "quality": "bun run lint && bun run format && bun run type-check",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:gh-pages": "next build && bun run post-build && touch out/.nojekyll",
    "deploy:gh-pages": "bun run build:gh-pages"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.19.1",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "^2.25.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.575.0",
    "next": "^15.3.1",
    "nodemailer": "^7.0.4",
    "posthog-js": "^1.239.1",
    "react": "19.2.4",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.2.4",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.59.0",
    "sanitize-html": "^2.17.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@next/eslint-plugin-next": "^15.3.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.15.12",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.6",
    "@types/sanitize-html": "^2.16.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8.5.6",
    "storybook": "^8.6.14",
    "tailwindcss": "3",
    "typescript": "^5.8.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}
