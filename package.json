{
  "name": "aagt-private-loans",
  "version": "3.67.2",
  "author": "AAGT Private Loans",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && bun run post-build",
    "post-build": "bun run copy-to-root",
    "copy-to-root": "node scripts/copy-to-root.js",
    "start": "next start",
    "clean": "rm -rf .next out node_modules",
    "check-types": "tsc --noEmit --pretty",
    "export": "next build",
    "fresh-install": "bun run clean && bun install",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "next": "^15.3.1",
    "posthog-js": "^1.256.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.3.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^22.16.0",
    "@types/react": "^19.1.3",
    "eslint": "^9.30.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}
